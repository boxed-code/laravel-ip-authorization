{
    "name": "boxed-code/laravel-ip-authorization",
    "description": "Authorize access based on IPv4 & IPv6 addresses, ranges, subnets or wildcards.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "green2go@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "s1lentium/iptools": "^1.1",
        "laravel/ui": "^2.0|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BoxedCode\\Laravel\\Auth\\Ip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoxedCode\\Laravel\\Auth\\Ip\\IpAuthServiceProvider"
            ]
        }
    }
}
